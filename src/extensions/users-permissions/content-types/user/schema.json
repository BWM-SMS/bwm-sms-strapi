{
  "kind": "collectionType",
  "collectionName": "up_users",
  "info": {
    "name": "user",
    "description": "",
    "singularName": "user",
    "pluralName": "users",
    "displayName": "User"
  },
  "options": {
    "draftAndPublish": false
  },
  "attributes": {
    "username": {
      "type": "string",
      "minLength": 3,
      "unique": true,
      "configurable": false,
      "required": true
    },
    "email": {
      "type": "email",
      "minLength": 6,
      "configurable": false,
      "required": true
    },
    "provider": {
      "type": "string",
      "configurable": false
    },
    "password": {
      "type": "password",
      "minLength": 6,
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "resetPasswordToken": {
      "type": "string",
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "confirmationToken": {
      "type": "string",
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "confirmed": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "blocked": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "role": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.role",
      "inversedBy": "users",
      "configurable": false
    },
    "englishName": {
      "type": "string",
      "required": true
    },
    "chineseName": {
      "type": "string"
    },
    "phoneNumber": {
      "type": "biginteger",
      "required": false,
      "max": "999999999999999",
      "min": "9999999"
    },
    "birthYear": {
      "type": "integer",
      "max": 2500,
      "required": true,
      "min": 1900
    },
    "postalCode": {
      "type": "integer",
      "max": 999999,
      "min": 9999
    },
    "classAttendanceDetails": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::class-attendance-detail.class-attendance-detail",
      "mappedBy": "username"
    },
    "userClasses": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::user-class.user-class",
      "mappedBy": "username"
    },
    "gender": {
      "type": "enumeration",
      "enum": [
        "Male",
        "Female"
      ],
      "required": true
    },
    "joinYear": {
      "type": "integer",
      "required": true
    },
    "image": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "acceptPDPA": {
      "type": "boolean",
      "required": false
    },
    "localization": {
      "type": "enumeration",
      "enum": [
        "English",
        "Chinese (中文)"
      ],
      "default": "English",
      "required": true
    },
    "hobby": {
      "type": "customField",
      "default": "[]",
      "options": [
        "Reading",
        "Singing"
      ],
      "customField": "plugin::multi-select.multi-select"
    },
    "skill": {
      "type": "customField",
      "default": "[]",
      "options": [
        "IT",
        "Drawing"
      ],
      "customField": "plugin::multi-select.multi-select"
    },
    "sus_feedbacks": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::sus-feedback.sus-feedback",
      "private": true,
      "mappedBy": "username"
    }
  }
}
