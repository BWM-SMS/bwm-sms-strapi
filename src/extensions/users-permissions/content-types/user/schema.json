{
  "kind": "collectionType",
  "collectionName": "up_users",
  "info": {
    "name": "user",
    "description": "",
    "singularName": "user",
    "pluralName": "users",
    "displayName": "User"
  },
  "options": {
    "draftAndPublish": false
  },
  "attributes": {
    "username": {
      "type": "string",
      "minLength": 3,
      "unique": true,
      "configurable": false,
      "required": true
    },
    "email": {
      "type": "email",
      "minLength": 6,
      "configurable": false,
      "required": false
    },
    "provider": {
      "type": "string",
      "configurable": false
    },
    "password": {
      "type": "password",
      "minLength": 6,
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "resetPasswordToken": {
      "type": "string",
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "confirmationToken": {
      "type": "string",
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "confirmed": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "blocked": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "role": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.role",
      "inversedBy": "users",
      "configurable": false
    },
    "englishName": {
      "type": "string",
      "required": true
    },
    "chineseName": {
      "type": "string"
    },
    "gender": {
      "type": "enumeration",
      "enum": [
        "Male",
        "Female"
      ],
      "required": false
    },
    "phoneNumber": {
      "type": "text"
    },
    "birthYear": {
      "type": "integer",
      "max": 2500,
      "required": false,
      "min": 1900
    },
    "joinYear": {
      "type": "integer",
      "required": true
    },
    "postalCode": {
      "type": "integer",
      "max": 999999,
      "min": 9
    },
    "hobby": {
      "type": "customField",
      "default": "[]",
      "options": [
        "Creative & Artistic Arts 创意与艺术",
        "Physical Activities & Sports 体育活动与运动",
        "Music & Performance 音乐与表演",
        "Learning & Intellectual Growth 学习与知识拓展",
        "Technology & Digital Skills 科技与数码技能",
        "Nature & Outdoor Exploration 自然与户外探险",
        "Food & Beverage Culinary Exploration 烹饪与饮品探索",
        "Collecting & Cultural Appreciation 收藏与文化欣赏"
      ],
      "customField": "plugin::multi-select.multi-select"
    },
    "skill": {
      "type": "customField",
      "default": "[]",
      "options": [
        "Administrative 行政",
        "Art & Culture 文化艺术",
        "Digital & Information 数码 & 资讯",
        "Education 教育",
        "Events & Exhibitions 活动 & 展览",
        "Facilities & Maintenance 设施维护&管理",
        "Finance and Accounting 财会",
        "Food & Beverage 食品和饮料",
        "Graphic & Design 平面设计",
        "Healthcare 医疗保健",
        "Language & Literature 语言与文学",
        "Management 管理",
        "Policy & Legal Consultancy 政策与法律咨询",
        "Service & Hospitality 服务与接待"
      ],
      "customField": "plugin::multi-select.multi-select"
    },
    "image": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "acceptPDPA": {
      "type": "boolean",
      "required": false
    },
    "localization": {
      "type": "enumeration",
      "enum": [
        "English",
        "Chinese",
        "Malay",
        "Tamil"
      ],
      "default": "English",
      "required": true
    },
    "classAttendanceDetails": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::class-attendance-detail.class-attendance-detail",
      "mappedBy": "username"
    },
    "userClasses": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::user-class.user-class",
      "mappedBy": "username"
    },
    "sus_feedbacks": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::sus-feedback.sus-feedback",
      "private": true,
      "mappedBy": "username"
    },
    "notificationServices": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::notification-service.notification-service",
      "mappedBy": "username"
    }
  }
}
