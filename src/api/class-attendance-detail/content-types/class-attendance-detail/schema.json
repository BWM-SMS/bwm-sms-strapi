{
  "kind": "collectionType",
  "collectionName": "class_attendance_details",
  "info": {
    "singularName": "class-attendance-detail",
    "pluralName": "class-attendance-details",
    "displayName": "Class Attendance Detail",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "classAttendance": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::class-attendance.class-attendance",
      "inversedBy": "classAttendanceDetails"
    },
    "username": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "classAttendanceDetails"
    },
    "isAttend": {
      "type": "boolean",
      "default": false,
      "required": true
    },
    "note": {
      "type": "text"
    },
    "rating": {
      "type": "integer",
      "max": 5,
      "min": 0
    },
    "position": {
      "type": "enumeration",
      "enum": [
        "A. 班长",
        "B. 副班长",
        "C. 关怀员",
        "D. 学员",
        "E. 旁听"
      ],
      "default": "D. 学员",
      "required": true
    }
  }
}
