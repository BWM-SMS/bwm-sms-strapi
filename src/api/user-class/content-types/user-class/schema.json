{
  "kind": "collectionType",
  "collectionName": "user_classes",
  "info": {
    "singularName": "user-class",
    "pluralName": "user-classes",
    "displayName": "User Class",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "username": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "userClasses"
    },
    "className": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::class.class",
      "inversedBy": "userClasses"
    },
    "isActive": {
      "type": "boolean",
      "default": true,
      "required": true
    },
    "position": {
      "type": "enumeration",
      "enum": [
        "A. 班长",
        "B. 副班长",
        "C. 关怀员",
        "D. 学员",
        "E. 旁听"
      ],
      "default": "D. 学员",
      "required": true
    },
    "joinDate": {
      "type": "date",
      "required": true
    },
    "note": {
      "type": "text"
    },
    "endDate": {
      "type": "date"
    }
  }
}
